    <body>
        <% if (ID%2==0) { %> 
            <% for (var i=1; i<=ID*2; i++){ %>
                <li> <%= i %> </li>
            <% } %>
        <% } else {%>
            <% for (var i=1; i<=ID; i++){ %>
                <li> <%= i %> </li>
            <% } %>
        <% } %>

    </body>
</html>